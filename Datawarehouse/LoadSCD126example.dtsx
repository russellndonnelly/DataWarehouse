    <DTS:Executable
      DTS:refId="Package\Type1_and_3_SCD_changes"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:Disabled="True"
      DTS:DTSID="{75D8D96C-97FE-49FE-9BF5-22C0062B0021}"
