    <DTS:Executable
      DTS:refId="Package\Load SCD attributes 1,2,6"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{2704C7E9-C457-4901-BB7B-E3E0FB317F13}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Load SCD attributes 1,2,6"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{491675FB-4A47-43C8-88D3-C6890C9E484C}"
          SQLTask:SqlStatementSource="/*********************************************************************************************&#xA;Purpose:&#x9;&#x9;inserts and updates for SCD attribute types: 1,2 and 6&#xA;Attribute&#x9;&#x9;SCD Type&#xA;[Title]&#x9;&#x9;&#x9;1&#xA;[FirstName]&#x9;&#x9;2&#xA;[MiddleName]&#x9;2&#xA;[LastName]&#x9;&#x9;2&#xA;[PhoneNumber]&#x9;6&#xA;[PhoneType]&#x9;&#x9;1&#xA;*********************************************************************************************/&#xA;USE AdventureWorksDWExtended&#xA;&#xA;/***********************************************************************&#xA;--run in ssis:&#xA;&#x9;Declare @PackageStartDate as datetime = ?&#xA;***********************************************************************/&#xA;/**********************************************************************/&#xA;--run in ssms:  &#xA;&#x9;Declare @PackageStartDate as datetime = getdate()&#xA;/**********************************************************************/&#xA;&#x9;Declare @CurrentExpirationDate as datetime = Convert(date,'12/31/9999')&#xA;&#xA;/*type 1 attribute change*********************************************************************/&#xA;&#x9;MERGE INTO AdventureWorksDWExtended.dbo.DimCustomer as tgt&#xA;&#x9;USING AdventureWorksDWExtended.stage.DimCustomer src &#xA;&#x9;&#x9;ON tgt.[BusinessEntityID_nk] = src.[BusinessEntityID_nk]&#xA;&#x9;WHEN MATCHED AND EXISTS&#x9; --business key exists and type1 or type3 attribute changes:&#xA;&#x9;(&#xA;&#x9;&#x9;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;&#x9;src.Title&#x9;&#x9;&#x9;&#x9;--type1&#xA;&#x9;&#x9;&#x9;&#x9;,src.PhoneType&#x9;&#x9;&#x9;--type1&#xA;&#x9;&#x9;&#x9;EXCEPT&#xA;&#x9;&#x9;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;&#x9;tgt.Title&#xA;&#x9;&#x9;&#x9;&#x9;,tgt.PhoneType&#xA;&#x9;)&#xA;&#x9;THEN UPDATE&#x9;SET &#xA;&#x9;&#x9;tgt.Title = src.Title&#xA;&#x9;&#x9;,tgt.PhoneType = src.PhoneType&#xA;&#x9;&#x9;,tgt.DateUpdated = @PackageStartDate;&#xA;&#xA;&#xA;/*attribute type 2 or type 6 changes need to be processed together since both could potentially occur on the same time adding new row**/&#xA;&#x9;Insert Into dbo.DimCustomer&#xA;&#x9;(&#xA;&#x9;&#x9;BusinessEntityID_nk&#xA;&#x9;&#x9;,Title&#xA;&#x9;&#x9;,FirstName&#xA;&#x9;&#x9;,MiddleName&#xA;&#x9;&#x9;,LastName&#xA;&#x9;&#x9;,PhoneNumber_curr&#xA;&#x9;&#x9;,PhoneNumber_prev&#xA;&#x9;&#x9;,PhoneType&#xA;&#x9;&#x9;,EffectiveDate&#xA;&#x9;&#x9;,ExpirationDate&#xA;&#x9;&#x9;,DateInserted&#xA;&#x9;&#x9;,DateUpdated&#xA;&#x9;&#x9;,IsCurrentRow&#xA;&#x9;)&#xA;&#x9;Select&#xA;&#x9;&#x9;BusinessEntityID_nk&#xA;&#x9;&#x9;,Title&#xA;&#x9;&#x9;,FirstName&#xA;&#x9;&#x9;,MiddleName&#xA;&#x9;&#x9;,LastName&#xA;&#x9;&#x9;,PhoneNumber_curr&#xA;&#x9;&#x9;,PhoneNumber_prev&#xA;&#x9;&#x9;,PhoneType&#xA;&#x9;&#x9;,EffectiveDate&#xA;&#x9;&#x9;,ExpirationDate&#xA;&#x9;&#x9;,DateInserted&#xA;&#x9;&#x9;,DateUpdated&#xA;&#x9;&#x9;,IsCurrentRow&#xA;&#x9;From&#xA;&#x9;(&#xA;&#x9;&#x9;MERGE INTO dbo.DimCustomer as tgt&#xA;&#x9;&#x9;USING stage.DimCustomer as src &#xA;&#x9;&#x9;&#x9;ON tgt.BusinessEntityID_nk = src.BusinessEntityID_nk&#xA;&#x9;&#x9;WHEN MATCHED&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--these rows become newest history&#xA;&#x9;&#x9;&#x9;AND tgt.ExpirationDate = @CurrentExpirationDate &#xA;&#x9;&#x9;&#x9;AND tgt.IsCurrentRow = 1 &#xA;&#x9;&#x9;&#x9;AND EXISTS&#xA;&#x9;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9;&#x9;Select &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;src.[PhoneNumber] as 'PhoneNumber_curr'&#x9;&#x9;&#x9;&#x9;--type 6 attribute&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,src.[MiddleName]&#xA;&#x9;&#x9;&#x9;&#x9;Except&#xA;&#x9;&#x9;&#x9;&#x9;Select&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; tgt.[PhoneNumber_curr]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,tgt.[MiddleName]&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--type 2 attribute&#xA;&#x9;&#x9;&#x9;)&#xA;&#x9;&#x9;THEN UPDATE SET&#xA;&#x9;&#x9;&#x9;tgt.IsCurrentRow = 0&#xA;&#x9;&#x9;&#x9;,tgt.ExpirationDate = DateAdd(minute,-1,src.ModifiedDate)&#xA;&#x9;&#x9;OUTPUT $Action ActionOut,&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--new current row&#xA;&#x9;&#x9;&#x9;src.BusinessEntityID_nk&#xA;&#x9;&#x9;&#x9;,src.Title&#xA;&#x9;&#x9;&#x9;,src.FirstName&#xA;&#x9;&#x9;&#x9;,src.MiddleName&#xA;&#x9;&#x9;&#x9;,src.LastName&#xA;&#x9;&#x9;&#x9;,src.PhoneNumber as 'PhoneNumber_curr'&#xA;&#x9;&#x9;&#x9;,INSERTED.PhoneNumber_curr as 'PhoneNumber_prev'&#x9;&#x9;&#x9;--there is no UPDATED table of rows, rows are deleted and reinserted so INSERTED works here&#xA;&#x9;&#x9;&#x9;,src.PhoneType&#xA;&#x9;&#x9;&#x9;,src.ModifiedDate as 'EffectiveDate'&#xA;&#x9;&#x9;&#x9;,@CurrentExpirationDate as 'ExpirationDate'&#xA;&#x9;&#x9;&#x9;--,Convert(date,@CurrentExpirationDate) as 'ExpirationDate&#xA;&#x9;&#x9;&#x9;,@PackageStartDate as 'DateInserted'&#xA;&#x9;&#x9;&#x9;,@PackageStartDate as 'DateUpdated'&#xA;&#x9;&#x9;&#x9;,Cast(1 as bit)&#x9; as 'IsCurrentRow'&#xA;&#x9;) as MergeOut&#xA;&#x9;WHERE MergeOut.ActionOut = 'UPDATE';&#xA;&#xA;/***********new business/durable key row added in source**************************/&#xA;&#x9;MERGE INTO dbo.DimCustomer as tgt&#xA;&#x9;USING stage.DimCustomer as src &#xA;&#x9;&#x9;ON tgt.BusinessEntityID_nk = src.BusinessEntityID_nk&#xA;&#x9;WHEN NOT MATCHED BY TARGET THEN INSERT&#xA;&#x9;(&#xA;&#x9;&#x9;[BusinessEntityID_nk]&#xA;&#x9;&#x9;,[Title]&#xA;&#x9;&#x9;,[FirstName]&#xA;&#x9;&#x9;,[MiddleName]&#xA;&#x9;&#x9;,[LastName]&#xA;&#x9;&#x9;,[PhoneNumber_curr]&#xA;&#x9;&#x9;,[PhoneNumber_prev]&#xA;&#x9;&#x9;,[PhoneType]&#xA;&#x9;&#x9;,[EffectiveDate]&#xA;&#x9;&#x9;,[ExpirationDate]&#xA;&#x9;&#x9;,[DateInserted]&#xA;&#x9;&#x9;,[DateUpdated]&#xA;&#x9;&#x9;,[IsCurrentRow]&#xA;&#x9;)&#xA;&#x9;VALUES&#xA;&#x9;(&#xA;&#x9;&#x9;&#x9;src.[BusinessEntityID_nk]&#xA;&#x9;&#x9;,src.[Title]&#xA;&#x9;&#x9;,src.[FirstName]&#xA;&#x9;&#x9;,src.[MiddleName]&#xA;&#x9;&#x9;,src.[LastName]&#xA;&#x9;&#x9;,src.[PhoneNumber]&#xA;&#x9;&#x9;,'0000000000'&#x9;&#x9;&#x9;--default phone number&#xA;&#x9;&#x9;,src.[PhoneType]&#xA;&#x9;&#x9;,src.ModifiedDate&#xA;&#x9;&#x9;,@CurrentExpirationDate&#xA;&#x9;&#x9;,@PackageStartDate&#xA;&#x9;&#x9;,@PackageStartDate&#xA;&#x9;&#x9;,Cast(1 as bit)&#xA;&#x9;);&#xA;&#xA;&#xA;&#x9;SELECT [RowCount] = COUNT(*)&#xA;&#x9;FROM dbo.DimCustomer&#xA;&#x9;WHERE &#xA;&#x9;&#x9;DateInserted = @PackageStartDate &#xA;&#x9;&#x9;OR &#xA;&#x9;&#x9;DateUpdated = @PackageStartDate;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="RowCount"
            SQLTask:DtsVariableName="User::RowCount" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
    </DTS:Executable>
