      DTS:ThreadHint="3">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{491675FB-4A47-43C8-88D3-C6890C9E484C}"
          SQLTask:SqlStatementSource="/*************************************************************************&#xA;Purpose: insert and update rows in SCD dimension for type 1 and 3 changes&#xA;*************************************************************************/&#xA;&#xA;USE AdventureWorksDWExtended&#xA;--sql to load DimCustomer where:&#xA;&#x9;/*&#xA;&#x9;[Title]&#x9;&#x9;&#x9;= Type 1&#xA;&#x9;[FirstName]&#x9;&#x9;= Type 2&#xA;&#x9;[MiddleName]&#x9;= Type 2&#xA;&#x9;[LastName]&#x9;&#x9;= Type 2&#xA;&#x9;[PhoneNumber]&#x9;= Type 3&#xA;&#x9;[PhoneType]&#x9;&#x9;= Type 1&#xA;&#x9;*/&#xA;/***********************************************************************/&#xA;--run in ssis:&#xA;&#x9;Declare @PackageStartDate as datetime = ?&#xA;/***********************************************************************/&#xA;/**********************************************************************&#xA;--run in ssms:  &#xA;&#x9;Declare @PackageStartDate as datetime = getdate()&#xA;**********************************************************************/&#xA;&#x9;Declare @CurrentExpirationDate as datetime =&#x9;getdate()&#x9;&#x9;--Convert(date,'12/31/9999') --use convert to date for daily load of records and getdate for anytime load&#xA;&#xA;&#x9;--Type 1 and 3 updates and inserts&#xA;&#x9;MERGE INTO AdventureWorksDWExtended.dbo.DimCustomer as tgt&#xA;&#x9;USING AdventureWorksDWExtended.stage.DimCustomer src &#xA;&#x9;&#x9;ON tgt.[BusinessEntityID_nk] = src.[BusinessEntityID_nk]&#xA;&#x9;WHEN MATCHED AND tgt.PhoneNumber_Curr &lt;&gt; src.PhoneNumber AND EXISTS&#x9; --business key exists and type1 or type3 column changes:&#xA;&#x9;(&#xA;&#x9;&#x9;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;&#x9;src.PhoneNumber&#x9;&#x9;&#x9;--type3&#xA;&#x9;&#x9;&#x9;&#x9;,src.Title&#x9;&#x9;&#x9;&#x9;--type1&#xA;&#x9;&#x9;&#x9;&#x9;,src.PhoneType&#x9;&#x9;&#x9;--type1&#xA;&#x9;&#x9;&#x9;EXCEPT&#xA;&#x9;&#x9;&#x9;SELECT&#xA;&#x9;&#x9;&#x9;&#x9;tgt.PhoneNumber_curr&#xA;&#x9;&#x9;&#x9;&#x9;,tgt.Title&#xA;&#x9;&#x9;&#x9;&#x9;,tgt.PhoneType&#xA;&#x9;)&#xA;&#x9;THEN UPDATE&#x9;SET &#xA;&#x9;&#x9;tgt.PhoneNumber_prev = tgt.PhoneNumber_curr,&#xA;&#x9;&#x9;tgt.PhoneNumber_curr = src.PhoneNumber&#xA;&#x9;&#x9;--tgt.Title = src.Title&#xA;&#x9;&#x9;--,tgt.PhoneNumber_curr = src.PhoneNumber&#xA;&#x9;&#x9;--,tgt.PhoneNumber_prev = &#xA;&#x9;&#x9;--&#x9;CASE &#xA;&#x9;&#x9;--&#x9;WHEN &#xA;&#x9;&#x9;--&#x9;&#x9;(src.PhoneNumber &lt;&gt; tgt.PhoneNumber_curr)&#x9;&#x9;&#x9;--type 3&#xA;&#x9;&#x9;--&#x9;&#x9;and&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--type 6, for type 3 only remove this&#xA;&#x9;&#x9;--&#x9;&#x9;(tgt.IsCurrentRow = 1)&#xA;&#x9;&#x9;--&#x9;THEN tgt.PhoneNumber_curr&#xA;&#x9;&#x9;--&#x9;WHEN &#xA;&#x9;&#x9;--&#x9;&#x9;(tgt.PhoneNumber_curr &lt;&gt; tgt.PhoneNumber_prev)&#x9;&#x9;&#x9;--type 3&#xA;&#x9;&#x9;--&#x9;&#x9;and&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--type 6, for type 3 only remove this&#xA;&#x9;&#x9;--&#x9;&#x9;(tgt.IsCurrentRow = 0)&#xA;&#x9;&#x9;--&#x9;THEN tgt.PhoneNumber_curr&#xA;&#x9;&#x9;--&#x9;ELSE tgt.PhoneNumber_prev&#xA;&#x9;&#x9;--&#x9;END&#xA;&#x9;&#x9;,tgt.PhoneType = src.PhoneType&#xA;&#x9;&#x9;,tgt.DateUpdated = @PackageStartDate;&#xA;&#xA;&#x9;SELECT [RowCount] = COUNT(*)&#xA;&#x9;FROM dbo.DimCustomer&#xA;&#x9;WHERE &#xA;&#x9;&#x9;DateInserted = @PackageStartDate &#xA;&#x9;&#x9;OR &#xA;&#x9;&#x9;DateUpdated = @PackageStartDate;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
