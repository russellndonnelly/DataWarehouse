          SQLTask:Connection="{491675FB-4A47-43C8-88D3-C6890C9E484C}"
          SQLTask:SqlStatementSource="--test SCD, make sure parameter for ServerName != 'prodserver' and Reload = false&#xA;--1) Run package to initially load DimCustomer&#xA;/**************************************************************/&#xA;&#xA;&#xA;--2) Run this to check initial load:&#xA;SELECT * FROM [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;Where BusinessEntityID_nk in (29484,29485,29486)&#xA;Order By BusinessEntityID_nk&#xA;&#xA;--3) Verify load to DimCustomer:&#xA;/**************************************************************/&#xA;SELECT * FROM [AdventureWorksDWExtended].[dbo].[DimCustomer]&#xA;Where BusinessEntityID_nk in (29484,29485,29486)&#xA;Order By BusinessEntityID_nk, EffectiveDate&#xA;&#xA;SELECT TOP (1) * FROM [AdventureWorksDWExtended].[dbo].[Audit] Order By AuditId desc&#xA;&#xA;/**************************************************************/&#xA;&#xA;--3) Updates to test function of SCD:&#xA;/*&#xA;UPDATE [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;SET&#xA;&#x9;Title = 'Mss'&#xA;&#x9;,MiddleName = 'X'&#xA;&#x9;,PhoneNumber = '3339991111'&#xA;&#x9;,ModifiedDate = GetDate()&#xA;WHERE BusinessEntityID_nk = '29484'&#xA;&#xA;UPDATE [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;SET&#xA;&#x9;Title = 'Mss'&#xA;&#x9;,MiddleName = 'X'&#xA;&#x9;,PhoneNumber = '2229991111'&#xA;&#x9;,ModifiedDate = GetDate()&#xA;WHERE BusinessEntityID_nk = '29485'&#xA;&#xA;UPDATE [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;SET&#xA;&#x9;Title = 'Mss'&#xA;&#x9;,MiddleName = 'X'&#xA;&#x9;,PhoneNumber = '1119991111'&#xA;&#x9;,ModifiedDate = GetDate()&#xA;WHERE BusinessEntityID_nk = '29486'&#xA;*/&#x9;&#xA;/**************************************************************/&#xA;--4) Disable task: 'Recreate stage' table and dataflow: 'Load Stage'&#xA;&#xA;--5) Check load of changed records:&#xA;SELECT * FROM [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;Where BusinessEntityID_nk in (29484,29485,29486)&#xA;Order By BusinessEntityID_nk&#xA;&#xA;/**************************************************************/&#xA;&#xA;--6) Run package LoadSCDtemplate.dtsx&#xA;&#xA;--7) Verify changes to DimCustomer:&#xA;/**************************************************************/&#xA;&#xA;SELECT * FROM [AdventureWorksDWExtended].[dbo].[DimCustomer]&#xA;Where BusinessEntityID_nk in (29484,29485,29486)&#xA;Order By BusinessEntityID_nk, EffectiveDate&#xA;&#xA;&#xA;/*&#xA;UPDATE [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;SET&#xA;&#x9;PhoneNumber = '3339992222'&#xA;&#x9;,ModifiedDate = GetDate()&#xA;WHERE BusinessEntityID_nk = '29484'&#xA;&#xA;UPDATE [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;SET&#xA;&#x9;PhoneNumber = '2229992222'&#xA;&#x9;,ModifiedDate = GetDate()&#xA;WHERE BusinessEntityID_nk = '29485'&#xA;&#xA;UPDATE [AdventureWorksDWExtended].[stage].[DimCustomer]&#xA;SET&#xA;&#x9;PhoneNumber = '1119992222'&#xA;&#x9;,ModifiedDate = GetDate()&#xA;WHERE BusinessEntityID_nk = '29486'&#xA;*/&#x9;&#xA;&#xA;&#xA;/**************************************************************/&#xA;&#xA;SELECT * FROM [AdventureWorksDWExtended].[dbo].[DimCustomer]&#xA;Where BusinessEntityID_nk in (29484,29485,29486)&#xA;Order By BusinessEntityID_nk, EffectiveDate" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
