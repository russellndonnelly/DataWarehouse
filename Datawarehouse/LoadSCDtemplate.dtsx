      DTS:ObjectName="Type2 SCD changes"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{491675FB-4A47-43C8-88D3-C6890C9E484C}"
          SQLTask:SqlStatementSource="/****************************&#xA;Purpose:&#x9;&#x9;inserts and updates for SCD dimension type 2 changes&#xA;&#xA;&#x9;[Title]&#x9;&#x9;&#x9;= Type 1&#xA;&#x9;[FirstName]&#x9;&#x9;= Type 2&#xA;&#x9;[MiddleName]&#x9;= Type 2&#xA;&#x9;[LastName]&#x9;&#x9;= Type 2&#xA;&#x9;[PhoneNumber]&#x9;= Type 3&#xA;&#x9;[PhoneType]&#x9;&#x9;= Type 1&#xA;************************************************************************************************************/&#xA;USE AdventureWorksDWExtended&#xA;&#xA;/***********************************************************************/&#xA;--run in ssis:&#xA;&#x9;Declare @PackageStartDate as datetime = ?&#xA;/***********************************************************************/&#xA;/**********************************************************************&#xA;--run in ssms:  &#xA;&#x9;Declare @PackageStartDate as datetime = getdate()&#xA;**********************************************************************/&#xA;&#x9;Declare @CurrentExpirationDate as datetime = Convert(date,'12/31/9999')&#xA;&#x9;Declare @RowsProcessed as int;&#xA;&#x9;&#xA;&#x9;IF OBJECT_ID('tempdb..#DimCustomertemp') IS NOT NULL&#xA;&#x9;&#x9;DROP TABLE #DimCustomertemp;&#xA;&#xA;&#x9;CREATE TABLE #DimCustomertemp&#xA;&#x9;(&#xA;&#x9;&#x9;[BusinessEntityID_nk] [int] NOT NULL,&#xA;&#x9;&#x9;[Title] [nvarchar](50) NULL,&#xA;&#x9;&#x9;[FirstName] [nvarchar](50) NULL,&#xA;&#x9;&#x9;[MiddleName] [nvarchar](50) NULL,&#xA;&#x9;&#x9;[LastName] [nvarchar](50) NULL,&#xA;&#x9;&#x9;[PhoneNumber_curr] [nvarchar](25) NULL,&#xA;&#x9;&#x9;[PhoneNumber_prev] [nvarchar](25) NULL,&#xA;&#x9;&#x9;[PhoneType] [nvarchar](50) NULL,&#xA;&#x9;&#x9;[EffectiveDate] [datetime] NULL,&#xA;&#x9;&#x9;[ExpirationDate] [datetime] NULL,&#xA;&#x9;&#x9;[DateInserted] [datetime] NULL,&#xA;&#x9;&#x9;[DateUpdated] [datetime] NULL,&#xA;&#x9;&#x9;[IsCurrentRow] [bit] NULL&#xA;&#x9;);&#xA;&#xA;&#x9;--Type 2 inserts and updates&#xA;&#x9;INSERT INTO #DimCustomertemp&#xA;&#x9;(&#xA;&#x9;&#x9;[BusinessEntityID_nk]&#xA;&#x9;&#x9;,[Title]&#xA;&#x9;&#x9;,[FirstName]&#xA;&#x9;&#x9;,[MiddleName]&#xA;&#x9;&#x9;,[LastName] &#xA;&#x9;&#x9;,[PhoneNumber_curr]&#xA;&#x9;&#x9;,[PhoneNumber_prev]&#xA;&#x9;&#x9;,[PhoneType]&#xA;&#x9;&#x9;,[EffectiveDate]&#xA;&#x9;&#x9;,[ExpirationDate]&#xA;&#x9;&#x9;,[DateInserted]&#xA;&#x9;&#x9;,[DateUpdated]&#xA;&#x9;&#x9;,[IsCurrentRow]&#xA;&#x9;)&#xA;&#x9;SELECT &#xA;&#x9;&#x9;[BusinessEntityID_nk]&#xA;&#x9;&#x9;,[Title]&#xA;&#x9;&#x9;,[FirstName]&#xA;&#x9;&#x9;,[MiddleName]&#xA;&#x9;&#x9;,[LastName]&#xA;&#x9;&#x9;,[PhoneNumber_curr]&#xA;&#x9;&#x9;,[PhoneNumber_prev]&#xA;&#x9;&#x9;,[PhoneType]&#xA;&#x9;&#x9;,[EffectiveDate]&#xA;&#x9;&#x9;,[ExpirationDate]&#xA;&#x9;&#x9;,[DateInserted]&#xA;&#x9;&#x9;,[DateUpdated]&#xA;&#x9;&#x9;,[IsCurrentRow]&#xA;    FROM &#xA;&#x9;(&#xA;&#x9;&#x9;MERGE INTO dbo.DimCustomer as tgt&#xA;&#x9;&#x9;USING stage.DimCustomer as src &#xA;&#x9;&#x9;&#x9;ON tgt.BusinessEntityID_nk = src.BusinessEntityID_nk&#xA;&#x9;&#x9;WHEN MATCHED AND tgt.[ExpirationDate] = @CurrentExpirationDate AND EXISTS &#xA;&#x9;&#x9;(&#x9;&#xA;&#x9;&#x9;&#x9;SELECT &#xA;&#x9;&#x9;&#x9;&#x9;src.[FirstName]&#x9;&#x9;-- Type 2&#xA;&#x9;&#x9;&#x9;&#x9;,src.[MiddleName]&#x9;-- Type 2&#xA;&#x9;&#x9;&#x9;&#x9;,src.[LastName]&#x9;&#x9;-- Type 2&#xA;&#x9;&#x9;&#x9;EXCEPT &#xA;&#x9;&#x9;&#x9;SELECT &#xA;&#x9;&#x9;&#x9;&#x9;tgt.[FirstName]&#xA;&#x9;&#x9;&#x9;&#x9;,tgt.[MiddleName]&#xA;&#x9;&#x9;&#x9;&#x9;,tgt.[LastName]&#xA;&#x9;&#x9;)&#xA;&#x9;&#x9;THEN UPDATE&#xA;&#x9;&#x9;SET &#xA;&#x9;&#x9;&#x9;tgt.[ExpirationDate] = DateAdd(minute,-1,src.ModifiedDate),&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--expired date of row is yesterday from modified date from stage.DimCustomer, anytime run&#xA;&#x9;&#x9;&#x9;--tgt.[ExpirationDate] = Convert(date,DateAdd(day,-1,src.ModifiedDate)),&#x9;&#x9;--expired date of row is yesterday from modified date from stage.DimCustomer, daily run only&#xA;&#x9;&#x9;&#x9;tgt.[DateUpdated] = @PackageStartDate,&#xA;&#x9;&#x9;&#x9;tgt.[IsCurrentRow] = Cast(0 as bit)&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--set historical row IsCurrentRow = 0&#xA;&#x9;&#x9;WHEN NOT MATCHED BY TARGET THEN INSERT&#xA;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9;[BusinessEntityID_nk]&#xA;&#x9;&#x9;&#x9;,[Title]&#xA;&#x9;&#x9;&#x9;,[FirstName]&#xA;&#x9;&#x9;&#x9;,[MiddleName]&#xA;&#x9;&#x9;&#x9;,[LastName]&#xA;&#x9;&#x9;&#x9;,[PhoneNumber_curr]&#xA;&#x9;&#x9;&#x9;,[PhoneNumber_prev]&#xA;&#x9;&#x9;&#x9;,[PhoneType]&#xA;&#x9;&#x9;&#x9;,[EffectiveDate]&#xA;&#x9;&#x9;&#x9;,[ExpirationDate]&#xA;&#x9;&#x9;&#x9;,[DateInserted]&#xA;&#x9;&#x9;&#x9;,[DateUpdated]&#xA;&#x9;&#x9;&#x9;,[IsCurrentRow]&#xA;&#x9;&#x9;)&#xA;&#x9;&#x9;VALUES&#xA;&#x9;&#x9;(&#xA;&#x9;&#x9;&#x9; src.[BusinessEntityID_nk]&#xA;&#x9;&#x9;&#x9;,src.[Title]&#xA;&#x9;&#x9;&#x9;,src.[FirstName]&#xA;&#x9;&#x9;&#x9;,src.[MiddleName]&#xA;&#x9;&#x9;&#x9;,src.[LastName]&#xA;&#x9;&#x9;&#x9;,src.[PhoneNumber]&#xA;&#x9;&#x9;&#x9;,'0000000000'&#xA;&#x9;&#x9;&#x9;,src.[PhoneType]&#xA;&#x9;&#x9;&#x9;,src.ModifiedDate&#xA;&#x9;&#x9;&#x9;,@CurrentExpirationDate&#xA;&#x9;&#x9;&#x9;,@PackageStartDate&#xA;&#x9;&#x9;&#x9;,@PackageStartDate&#xA;&#x9;&#x9;&#x9;,Cast(1 as bit)&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--set current row IsCurrentRow = 1&#xA;&#x9;&#x9;)&#xA;&#x9;&#x9;OUTPUT $ACTION ActionOut, &#xA;&#x9;&#x9;&#x9;src.[BusinessEntityID_nk]&#xA;&#x9;&#x9;&#x9;,src.[Title]&#xA;&#x9;&#x9;&#x9;,src.[FirstName]&#xA;&#x9;&#x9;&#x9;,src.[MiddleName]&#xA;&#x9;&#x9;&#x9;,src.[LastName]&#xA;&#x9;&#x9;&#x9;,src.[PhoneNumber] as 'PhoneNumber_curr'&#xA;&#x9;&#x9;&#x9;,INSERTED.PhoneNumber_prev as 'PhoneNumber_prev'&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--WHEN MATCHED row&#xA;&#x9;&#x9;&#x9;,src.[PhoneType]&#xA;&#x9;&#x9;&#x9;,src.[ModifiedDate] as 'EffectiveDate'&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--anytime process&#xA;&#x9;&#x9;&#x9;--,Convert(date,src.[ModifiedDate]) as 'EffectiveDate'&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--daily process&#xA;&#x9;&#x9;&#x9;,@CurrentExpirationDate as 'ExpirationDate'&#xA;&#x9;&#x9;&#x9;,@PackageStartDate as 'DateInserted'&#xA;&#x9;&#x9;&#x9;,@PackageStartDate as 'DateUpdated'&#xA;&#x9;&#x9;&#x9;,Cast(1 as bit) as 'IsCurrentRow'&#xA;&#x9;) AS MergeOut&#xA;&#x9;WHERE  MergeOut.ActionOut = 'UPDATE';&#xA;&#xA;&#x9;/*&#xA;&#x9;We use a temp table, and then insert into the dimension table, to avoid this error message:&#xA;&#x9;&#x9;&quot;The target table 'TableName' of the INSERT statement cannot be on either side of a (primary key, foreign key) relationship &#xA;&#x9;&#x9;when the FROM clause contains a nested INSERT, UPDATE, DELETE, or MERGE statement. Found reference constraint 'ConstraintName'.&quot;&#xA;&#xA;&#x9;This only applies if the dimension table has foreign key relationships defined&#xA;&#x9;*/&#xA;&#x9;INSERT INTO dbo.DimCustomer&#xA;&#x9;(&#xA;&#x9;&#x9;[BusinessEntityID_nk]&#xA;&#x9;&#x9;,[Title]&#xA;&#x9;&#x9;,[FirstName]&#xA;&#x9;&#x9;,[MiddleName]&#xA;&#x9;&#x9;,[LastName] &#xA;&#x9;&#x9;,[PhoneNumber_curr]&#xA;&#x9;&#x9;,[PhoneNumber_prev] &#xA;&#x9;&#x9;,[PhoneType]&#xA;&#x9;&#x9;,[EffectiveDate]&#xA;&#x9;&#x9;,[ExpirationDate]&#xA;&#x9;&#x9;,[DateInserted]&#xA;&#x9;&#x9;,[DateUpdated]&#xA;&#x9;&#x9;,[IsCurrentRow]&#xA;&#x9;)&#xA;&#x9;SELECT &#xA;&#x9;&#x9;[BusinessEntityID_nk]&#xA;&#x9;&#x9;,[Title]&#xA;&#x9;&#x9;,[FirstName]&#xA;&#x9;&#x9;,[MiddleName]&#xA;&#x9;&#x9;,[LastName]&#xA;&#x9;&#x9;,[PhoneNumber_curr]&#xA;&#x9;&#x9;,[PhoneNumber_prev]&#xA;&#x9;&#x9;,[PhoneType]&#xA;&#x9;&#x9;,[EffectiveDate]&#xA;&#x9;&#x9;,[ExpirationDate]&#xA;&#x9;&#x9;,[DateInserted]&#xA;&#x9;&#x9;,[DateUpdated]&#xA;&#x9;&#x9;,[IsCurrentRow]&#xA;&#x9;FROM #DimCustomertemp;&#xA;&#xA;&#x9;IF OBJECT_ID('tempdb..#DimCustomertemp') IS NOT NULL&#xA;    DROP TABLE #DimCustomertemp;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="0"
            SQLTask:DtsVariableName="System::StartTime"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="7"
            SQLTask:ParameterSize="-1" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
    </DTS:Executable>
